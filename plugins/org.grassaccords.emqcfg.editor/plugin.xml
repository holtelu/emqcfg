<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.grassaccords.emqcfg.editor.MqcfgEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="mqcfg"
            icon="icons/file.gif"
            id="org.grassaccords.emqcfg.editor.MqcfgEditor"
            name="mqcfg Editor">
      </editor>
   </extension>

   <extension
         point="org.eclipse.ui.newWizards">
         
		<wizard
			category="org.openarchitectureware.xtext.wizards"
			class="org.grassaccords.emqcfg.wizards.NewMqcfgProjectWizard"
			icon="icons/template.gif"
			id="org.grassaccords.emqcfg.wizards.NewMqcfgProjectWizard"
			name="mqcfg Project"
			project="true">
		</wizard>
         
		<wizard
			category="org.openarchitectureware.xtext.wizards"
			class="org.grassaccords.emqcfg.wizards.NewMqcfgFileWizard"
			icon="icons/file.gif"
			id="org.grassaccords.emqcfg.wizards.NewMqcfgFileWizard"
			name="mqcfg Editor file">
		</wizard>
	</extension>
</plugin>
